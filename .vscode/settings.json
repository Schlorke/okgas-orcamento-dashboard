{
  "cSpell.language": "pt,pt-BR,en",
  "cSpell.allowCompoundWords": true,
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.checkOnlyEnabledFileTypes": true,
  "cSpell.enabledLanguageIds": [
    "markdown",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "yaml",
    "yml",
    "html",
    "css",
    "scss",
    "less",
    "plaintext"
  ],
  "cSpell.enableFiletypes": [
    "markdown",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "yaml",
    "yml",
    "html",
    "css",
    "scss",
    "less"
  ],
  "cSpell.dictionaries": [
    "portuguese",
    "pt_BR",
    "en_US",
    "typescript",
    "node",
    "npm",
    "html",
    "css",
    "companies",
    "softwareTerms",
    "okgas-terms"
  ],
  "markdownlint.config": {
    "MD001": false,
    "MD013": false,
    "MD022": false,
    "MD025": false,
    "MD033": false,
    "MD034": false,
    "MD036": false,
    "MD041": false
  },
  "markdown.validate.enabled": false,
  "markdown.extension.toc.levels": "1..6",
  "markdown.extension.completion.root": "./",
  "files.associations": {
    "*.md": "markdown"
  },
  "files.exclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true
  },
  "markdownlint.ignore": [
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**",
    "**/build/**"
  ],
