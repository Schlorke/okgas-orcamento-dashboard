{
  "version": "0.2",
  "language": "pt,pt-BR,en",
  "allowCompoundWords": true,
  "dictionaries": [
    "portuguese",
    "pt_BR",
    "en_US",
    "typescript",
    "node",
    "npm",
    "html",
    "css",
    "companies",
    "softwareTerms",
    "typescript",
    "react"
  ],
  "dictionaryDefinitions": [
    {
      "name": "okgas-terms",
      "path": "./cspell-okgas.txt",
      "description": "Termos específicos do projeto OK Gás Engenharia"
    }
  ],
  "ignorePaths": [
    "node_modules/**",
    ".next/**",
    "dist/**",
    "build/**",
    "coverage/**",
    ".git/**",
    "*.min.js",
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock"
  ],
  "ignoreRegExpList": [
    "/\\b[A-F0-9]{32}\\b/g",
    "/\\b[A-F0-9]{40}\\b/g",
    "/\\b[A-F0-9]{64}\\b/g",
    "/\\bhttps?:\\/\\/[^\\s]+/g",
    "/\\b\\w*[0-9]\\w*\\b/g"
  ],
  "words": [
    "acompanhamento",
    "acumulado",
    "animações",
    "apenas",
    "aplicar",
    "após",
    "arquitetura",
    "autenticação",
    "automatização",
    "câmera",
    "características",
    "clsx",
    "combina",
    "cronogramas",
    "customizada",
    "dashboard",
    "dependências",
    "desenvolvimento",
    "elementos",
    "entrada",
    "escalável",
    "executiva",
    "executivo",
    "exportação",
    "foco",
    "funcionalidade",
    "funcionalidades",
    "fundo",
    "geist",
    "gerenciamento",
    "gestores",
    "gráficos",
    "html2canvas",
    "implementação",
    "indicadores",
    "inicial",
    "instalação",
    "intuitiva",
    "jspdf",
    "okgas",
    "Okgas",
    "lucide",
    "luzes",
    "mais",
    "mapeamento",
    "métricas",
    "nextjs",
    "onclone",
    "otimizado",
    "pnpm",
    "pré",
    "profissional",
    "progresso",
    "qualidade",
    "recharts",
    "requisitos",
    "roadmap",
    "shadcn",
    "sofisticada",
    "solução",
    "substep",
    "substeps",
    "tailwindcss",
    "tecnológica",
    "troubleshooting",
    "typescript",
    "unificada",
    "vercel",
    "visualizações"
  ],
  "overrides": [
    {
      "filename": "**/*.md",
      "language": "pt,pt-BR,en"
    },
    {
      "filename": "**/*.{ts,tsx,js,jsx}",
      "language": "en,pt,pt-BR"
    }
  ]
}
